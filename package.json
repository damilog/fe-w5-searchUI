{
  "name": "fe-w3-shopping",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch-scss": "sass --watch public/stylesheets/style.scss:public/stylesheets/style.css",
    "server-start": "nodemon ./server.js",
    "start": "concurrently --kill-other-on-fail \"npm run watch-scss\" \"npm run server-start\"",
    "start-p": "parcel ./views/index.html --open",
    "build:sass": "sass stylesheets/scss:dist/css",
    "on": "npx parcel views/index.html --open"
  },
  "dependencies": {
    "concurrently": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0"
  },
  "description": "진행중",
  "main": "app.js",
  "devDependencies": {
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "parcel-bundler": "1.12.3",
    "prettier": "^2.2.1",
    "sass": "^1.32.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damilog/fe-w3-shopping.git"
  },
  "keywords": [],
